Date: Fri, 11 Sep 2015 12:15:05 +0200
Subject: [PATCH] fix fdt_addr for imx6qdl and default dtb name

---
 include/configs/common_parameter.h |    2 +-
 include/configs/icorem6dl.h        |    4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/include/configs/common_parameter.h b/include/configs/common_parameter.h
index 20b54f2..f3da544 100644
--- a/include/configs/common_parameter.h
+++ b/include/configs/common_parameter.h
@@ -67,7 +67,7 @@
 		"mode=YOCTO_MODE\0"																	\
 		"mmcdev=0\0"																		\
 		"mmcpart=1\0"																		\
-		"fdt_addr=0x83000000\0" 																\
+		"fdt_addr=0x18000000\0" 																\
 
 	/* Customized parameter
 	 * Customized parameter for SODIMM iCore modules
diff --git a/include/configs/icorem6dl.h b/include/configs/icorem6dl.h
index d374bbc..327c7ef 100644
--- a/include/configs/icorem6dl.h
+++ b/include/configs/icorem6dl.h
@@ -39,9 +39,9 @@
 #define CONFIG_CMDLINE_EDITING
 
 #if defined(CONFIG_MX6Q)
-#define CONFIG_DEFAULT_FDT_FILE	"icorem6q.dtb"
+#define CONFIG_DEFAULT_FDT_FILE	"imx6q-icore.dtb"
 #elif defined(CONFIG_MX6DL)
-#define CONFIG_DEFAULT_FDT_FILE	"icorem6q.dtb"
+#define CONFIG_DEFAULT_FDT_FILE	"imx6dl-icore.dtb"
 #endif
 #define PHYS_SDRAM_SIZE		(256u * 1024 * 1024)
 
