From 9545a88edc2e477f6d573e36168954a8eba37400 Mon Sep 17 00:00:00 2001
From: "matteo.lisi" <matteo.lisi@engicam.com>
Date: Tue, 10 Nov 2015 15:49:14 +0100
Subject: [PATCH] remove power key form geam6ul

---
 arch/arm/boot/dts/imx6ul.dtsi | 8 --------
 1 file changed, 8 deletions(-)

diff --git a/arch/arm/boot/dts/imx6ul.dtsi b/arch/arm/boot/dts/imx6ul.dtsi
index d46408e..78f158e 100644
--- a/arch/arm/boot/dts/imx6ul.dtsi
+++ b/arch/arm/boot/dts/imx6ul.dtsi
@@ -680,14 +680,6 @@
 				};
 			};
 
-			snvs-pwrkey@0x020cc000 {
-				compatible = "fsl,imx6ul-snvs-pwrkey", "fsl,imx6sx-snvs-pwrkey";
-				reg = <0x020cc000 0x4000>;
-				interrupts = <GIC_SPI 4 IRQ_TYPE_LEVEL_HIGH>;
-				fsl,keycode = <116>; /* KEY_POWER */
-				fsl,wakeup;
-			};
-
 			epit1: epit@020d0000 {
 				reg = <0x020d0000 0x4000>;
 				interrupts = <GIC_SPI 56 IRQ_TYPE_LEVEL_HIGH>;
-- 
1.9.1

